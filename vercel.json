{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [{
            "src": "/(.*).html",
            "dest": "/$1.html"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/image/(.*).png",
            "dest": "/image/$1.png"
        },
        {
            "src": "/(.+)",
            "dest": "/"
        }
    ]
}